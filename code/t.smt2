(declare-const A-chief Bool)
(declare-const B-chief Bool)
(declare-const A-knight Bool)
(declare-const B-knight Bool)
(assert (distinct A-chief B-chief))
(assert (= A-knight (and B-chief (not B-knight))))
(assert (= B-knight (and (not A-chief) A-knight)))
(check-sat)
(get-value (A-knight B-knight A-chief B-chief))
